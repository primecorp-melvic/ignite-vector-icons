{
  "name": "ignite-rn-vector-icons",
  "description": "An Ignite plugin which installs react-native-vector-icons.",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "README.md",
    "plugin.js",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primecorp-melvic/ignite-vector-icons.git"
  },
  "author": "Infinite Red <npm@infinite.red> (https://github.com/infinitered/ignite)",
  "scripts": {
    "lint": "standard",
    "test": "ava",
    "watch": "ava --watch",
    "coverage": "nyc ava",
    "ci:publish": "yarn semantic-release",
    "semantic-release": "semantic-release"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.5",
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "nyc": "^10.1.2",
    "semantic-release": "^15.12.5",
    "sinon": "^1.17.7",
    "standard": "^8.6.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": "package.json",
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/infinitered/ignite-vector-icons/issues"
  },
  "homepage": "https://github.com/primecorp-melvic/ignite-vector-icons/blob/master/README.md",
  "main": "plugin.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "React",
    "Native",
    "Vector",
    "Icons",
    "Font",
    "Awesome",
    "Font",
    "Awesome",
    "5",
    "Material",
    "Icons"
  ]
}
